{
  "version": 2,
  "framework": "create-react-app",
  "routes": [
    {
      "src": "/api/proxy/(.*)",
      "dest": "/api/proxy",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/proxy.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}